{
    "sidebar": {
        "relatedObjects": {
            ".csc-recordList-row:": {
                "children": [
                    {
                        ".csc-related-summary": "${items.0.summary}",
                        ".csc-related-number": {
                            "linktext": "${items.0.number}",
                            "target": "${items.0.recordtype}.html?csid=${items.0.csid}"
                        }
                    }
                ]
            }
        },
        "relatedProcedures": {
            ".csc-recordList-row:": {
                "children": [
                    {
                        ".csc-related-summary": "${items.0.summary}",
                        ".csc-related-number": {
                            "linktext": "${items.0.number}",
                            "target": "${items.0.recordtype}.html?csid=${items.0.csid}"
                        },
                        ".csc-related-recordtype": "${items.0.recordtype}"
                    }
                ]
            }
        },
        "termsUsed": {
            ".csc-recordList-row:": {
                "children": [
                    {
                        ".csc-related-number": {
                            "linktext": "${items.0.number}",
                            "target": "${items.0.recordtype}.html?csid=${items.0.csid}"
                        },
                        ".csc-related-field": "${items.0.sourceFieldName}",
                        ".csc-related-recordtype": "${items.0.recordtype}"
                    }
                ]
            }
        }
    },
    "titleBar": {
        ".csc-titleBar-loanInNumber": "${fields.loanInNumber}"
    },
    "recordEditor": {
        ".csc-loanIn-loanInContact": "${fields.loanInContact}",
        ".csc-loanIn-lender-row:": {
            "children": [
                {
                    ".csc-loanIn-lendersAuthorizerDate-container": {
                        "decorators": [
                            {
                                "func": "cspace.datePicker",
                                "type": "fluid"
                            }
                        ]
                    },
                    ".csc-loanIn-lender": {
                        "valuebinding": "${fields.lenders.0.lender}",
                        "decorators": [
                            {
                                "func": "cspace.autocomplete",
                                "type": "fluid",
                                "options": {
                                    "queryUrl": "../../chain/loanin/autocomplete/lender",
                                    "vocabUrl": "../../chain/loanin/source-vocab/lender"
                                }
                            }
                        ]
                    },
                    ".csc-loanIn-lendersContact": {
                        "valuebinding": "${fields.lenders.0.lendersContact}",
                        "decorators": [
                            {
                                "func": "cspace.autocomplete",
                                "type": "fluid",
                                "options": {
                                    "queryUrl": "../../chain/loanin/autocomplete/lendersContact",
                                    "vocabUrl": "../../chain/loanin/source-vocab/lendersContact"
                                }
                            }
                        ]
                    },
                    ".csc-loanIn-lendersAuthorizerDate": "${fields.lenders.0.lendersAuthorizerDate}",
                    ".csc-loanIn-lendersAuthorizer": {
                        "valuebinding": "${fields.lenders.0.lendersAuthorizer}",
                        "decorators": [
                            {
                                "func": "cspace.autocomplete",
                                "type": "fluid",
                                "options": {
                                    "queryUrl": "../../chain/loanin/autocomplete/lendersAuthorizer",
                                    "vocabUrl": "../../chain/loanin/source-vocab/lendersAuthorizer"
                                }
                            }
                        ]
                    }
                }
            ]
        },
        ".csc-loanIn-loanRenewalApplicationDate-container": {
            "decorators": [
                {
                    "func": "cspace.datePicker",
                    "type": "fluid"
                }
            ]
        },
        ".csc-loanIn-loanInDate-container": {
            "decorators": [
                {
                    "func": "cspace.datePicker",
                    "type": "fluid"
                }
            ]
        },
        ".csc-loanIn-loanInNumber-patternChooserContainer": {
            "decorators": [
                {
                    "func": "cspace.numberPatternChooser",
                    "type": "fluid",
                    "options": {
                        "model": {
                            "names": [
                                "Loan In"
                            ],
                            "list": [
                                "loanin"
                            ],
                            "samples": [
                                "LI2010.1"
                            ]
                        },
                        "selectors": {
                            "numberField": ".csc-loanIn-loanInNumber"
                        }
                    }
                }
            ]
        },
        ".csc-loanIn-loanInNote": "${fields.loanInNote}",
        ".csc-loanIn-loanReturnDate": "${fields.loanReturnDate}",
        ".csc-loanIn-loanReturnDate-container": {
            "decorators": [
                {
                    "func": "cspace.datePicker",
                    "type": "fluid"
                }
            ]
        },
        ".csc-loanIn-loanInDate": "${fields.loanInDate}",
        ".csc-loanIn-loanInConditions": "${fields.loanInConditions}",
        ".csc-loanIn-loanInNumber": "${fields.loanInNumber}",
        ".csc-loanIn-loanRenewalApplicationDate": "${fields.loanRenewalApplicationDate}",
        ".csc-loanIn-loanInContact-autocomplete": {
            "decorators": [
                {
                    "func": "cspace.autocomplete",
                    "type": "fluid",
                    "options": {
                        "queryUrl": "../../chain/loanin/autocomplete/loanInContact",
                        "vocabUrl": "../../chain/loanin/source-vocab/loanInContact"
                    }
                }
            ]
        },
        ".csc-loanIn-loanPurpose": {
            "default": "0",
            "optionnames": [
                "Exhibition",
                "Research",
                "Scientific or exhibit preparation",
                "Analysis",
                "Photography",
                "Conservation or other requested services",
                "Long-term collections management and storage"
            ],
            "optionlist": [
                "exhibition",
                "research",
                "scientificorexhibitpreparation",
                "analysis",
                "photography",
                "conservationotherrequestedservices",
                "longtermcollectionsmanagementandstorage"
            ],
            "selection": "${fields.loanPurpose}"
        }
    }
}