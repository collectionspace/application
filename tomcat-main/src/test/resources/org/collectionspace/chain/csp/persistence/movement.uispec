{"recordEditor": {
    ".csc-movement-movementReferenceNumber-label": {"messagekey": "movement-movementReferenceNumberLabel"},
    ".csc-movement-coreInformation-label": {"messagekey": "movement-coreInformationLabel"},
    ".csc-movement-inventoryContactList-label": {"messagekey": "movement-inventoryContactListLabel"},
    ".csc-movement-movementNote": "${fields.movementNote}",
    ".csc-movement-movementReferenceNumber-container": {"decorators": [{
        "func": "cspace.numberPatternChooser",
        "type": "fluid",
        "options": {
            "model": {
                "names": [
                    "Inventory",
                    "Location",
                    "Movement"
                ],
                "list": [
                    "inventory",
                    "location",
                    "movement"
                ],
                "samples": [
                    "INV2010.1",
                    "LOC2010.1",
                    "MV2010.1"
                ]
            },
            "selectors": {"numberField": ".csc-movement-movementReferenceNumber"}
        }
    }]},
    ".csc-movement-locationDate-label": {"messagekey": "movement-locationDateLabel"},
    ".csc-movement-plannedRemovalDate": {
        "decorators": [{
            "func": "cspace.datePicker",
            "type": "fluid"
        }],
        "value": "${fields.plannedRemovalDate}"
    },
    ".csc-movement-inventoryDate-label": {"messagekey": "movement-inventoryDateLabel"},
    ".csc-movement-createdBy-label": {"messagekey": "movement-createdByLabel"},
    ".csc-movement-inventoryContact": {"decorators": [{
        "func": "cspace.makeRepeatable",
        "type": "fluid",
        "options": {
            "repeatTree": {"expander": {
                "tree": {".csc-movement-inventoryContact": {
                    "decorators": [{
                        "func": "cspace.autocomplete",
                        "type": "fluid",
                        "options": {
                            "queryUrl": "../../../tenant/core/movement/autocomplete/inventoryContact",
                            "vocabUrl": "../../../tenant/core/movement/source-vocab/inventoryContact"
                        }
                    }],
                    "value": "${{row}.inventoryContact}"
                }},
                "type": "fluid.noexpand"
            }},
            "elPath": "fields.inventoryContactList"
        }
    }]},
    ".csc-movement-reasonForMove-label": {"messagekey": "movement-reasonForMoveLabel"},
    ".csc-movement-movementMethods": {"decorators": [{
        "func": "cspace.makeRepeatable",
        "type": "fluid",
        "options": {
            "repeatTree": {"expander": {
                "tree": {".csc-movement-movementMethods": {
                    "default": "",
                    "optionnames": [
                        "Please select a value",
                        "Forklift",
                        "Handcarried",
                        "Trolley"
                    ],
                    "optionlist": [
                        "",
                        "forklift",
                        "handcarried",
                        "trolley"
                    ],
                    "selection": "${{row}.movementMethod}"
                }},
                "type": "fluid.noexpand"
            }},
            "elPath": "fields.movementMethods"
        }
    }]},
    ".csc-movement-frequencyForInventory": {
        "default": "",
        "optionnames": [
            "Please select a value",
            "Daily",
            "Weekly",
            "Monthly",
            "Semi-annually",
            "Annually"
        ],
        "optionlist": [
            "",
            "daily",
            "weekly",
            "monthly",
            "semi-annually",
            "annually"
        ],
        "selection": "${fields.frequencyForInventory}"
    },
    ".csc-movement-currentLocationNote": "${fields.currentLocationNote}",
    ".csc-movement-nextInventoryDate": {
        "decorators": [{
            "func": "cspace.datePicker",
            "type": "fluid"
        }],
        "value": "${fields.nextInventoryDate}"
    },
    ".csc-movement-createdBy": "${fields.createdBy}",
    ".csc-movement-updatedAt": {
        "decorators": [{
            "func": "cspace.datePicker",
            "type": "fluid"
        }],
        "value": "${fields.updatedAt}"
    },
    ".csc-movement-inventoryNote": "${fields.inventoryNote}",
    ".csc-movement-workflow": "${fields.workflow}",
    ".csc-movement-currentLocationFitness-label": {"messagekey": "movement-currentLocationFitnessLabel"},
    ".csc-movement-createdAt-label": {"messagekey": "movement-createdAtLabel"},
    ".csc-movement-frequencyForInventory-label": {"messagekey": "movement-frequencyForInventoryLabel"},
    ".csc-movement-movementNote-label": {"messagekey": "movement-movementNoteLabel"},
    ".csc-movement-workflow-label": {"messagekey": "movement-workflowLabel"},
    ".csc-movement-domaindata-label": {"messagekey": "movement-domaindataLabel"},
    ".csc-movement-inventoryDate": {
