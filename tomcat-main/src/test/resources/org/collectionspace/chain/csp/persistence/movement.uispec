    ".csc-movement-inventoryActionRequired": {
        "default": "",
        "optionnames": [
            "Please select a value",
            "Conservation",
            "Preservation",
            "Re-housing"
        ],
        "optionlist": [
            "",
            "conservation",
            "preservation",
            "re-housing"
        ],
