    ".csc-acquisition-acquisitionFundingSource-label": {
        "messagekey": "acquisition-acquisitionFundingSourceLabel"
    },
    ".csc-acquisition-acquisitionFundingSource": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "hidePrimary": true,
                    "elPath": "acquisitionFunding",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-acquisition-acquisitionFundingSource": {
                                            "decorators": [
                                                {
                                                    "func": "cspace.autocomplete",
                                                    "type": "fluid",
                                                    "options": {
                                                        "queryUrl": "../../tenant/html/acquisition/autocomplete/acquisitionFundingSource",
                                                        "vocabUrl": "../../tenant/html/acquisition/source-vocab/acquisitionFundingSource"
                                                    }
                                                }
                                            ],
                                            "value": "${{row}.acquisitionFundingSource}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "acquisitionFunding"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }
        ]
    },    
    ".csc-acquisition-acquisitionMethod-label": {
        "messagekey": "acquisition-acquisitionMethodLabel"
    },
    ".csc-acquisition-acquisition-method": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "hidePrimary": true,
                    "elPath": "acquisitionMethods",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-acquisition-acquisitionMethod": {
                                            "default": "gift",
                                            "optionnames": [
                                                "Gift",
                                                "Purchase",
                                                "Exchange",
                                                "Transfer",
                                                "Treasure"
                                            ],
                                            "optionlist": [
                                                "gift",
                                                "purchase",
                                                "exchange",
                                                "transfer",
                                                "treasure"
                                            ],
                                            "selection": "${{row}.acquisitionMethod}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "acquisitionMethods"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }
        ]    
    },    
