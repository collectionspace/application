    "recordEditor": {
        ".csc-acquisition-object-purchase-price-currency": {
            "optionnames": ["Canadian Dollar", "Danish Krone", "Euro", "Please select a value", "Pound Sterling", "Swedish Krona", "Swiss Franc", "US Dollar"],
