{
  ".csc-acquisition-acquisition-method": {
    "decorators": [
      {
        "func": "cspace.makeRepeatable", 
        "options": {
          "elPath": "acquisitionMethods", 
          "hidePrimary": true, 
          "repeatTree": {
            "expander": {
              "tree": {
                ".csc-acquisition-acquisition-method": {
                  "default": "", 
                  "optionlist": [
                    "gift", 
                    "purchase", 
                    "exchange", 
                    "transfer", 
                    "treasure", 
                    ""
                  ], 
                  "optionnames": [
                    "Gift", 
                    "Purchase", 
                    "Exchange", 
                    "Transfer", 
                    "Treasure", 
                    "Please select a value"
                  ], 
                  "selection": "${{row}.acquisitionMethod}"
                }
              }, 
              "type": "fluid.noexpand"
            }
          }
        }, 
        "type": "fluid"
      }
    ]
  }, 
  ".csc-acquisition-acquisitionFunding": {
