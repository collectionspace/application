                    {
                      "func": "cspace.autocomplete", 
                      "options": {
                        "queryUrl": "../../../tenant/core/acquisition/autocomplete/acquisitionFundingSource", 
                        "vocabUrl": "../../../tenant/core/acquisition/source-vocab/acquisitionFundingSource"
                      }, 
                      "type": "fluid"
                    }
                  ], 
                  "value": "${{row}.acquisitionFundingSource}"
                }
              }, 
              "type": "fluid.noexpand"
            }
          }
        }, 
        "type": "fluid"
      }
    ]
  }, 
