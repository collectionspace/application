{
    "details": {
        ".csc-permrole-actionGroup-label": {
            "messagekey": "permrole-actionGroupLabel"
        },
        ".csc-permrole-permissionRoleInformation-label": {
            "messagekey": "permrole-permissionRoleInformationLabel"
        },
        ".csc-permrole-roleId-label": {
            "messagekey": "permrole-roleIdLabel"
        },
        ".csc-permrole-permissionId-label": {
            "messagekey": "permrole-permissionIdLabel"
        },
        ".csc-permrole-role-label": {
            "messagekey": "permrole-roleLabel"
        },
        ".csc-permrole-roleName-label": {
            "messagekey": "permrole-roleNameLabel"
        },
        ".csc-permrole-resourceName-label": {
            "messagekey": "permrole-resourceNameLabel"
        },
        ".csc-permrole-role": {
            ".csc-permrole-roleId": "${{row}.roleId}",
            ".csc-permrole-roleName": "${{row}.roleName}"
        },
        ".csc-permrole-domaindata-label": {
            "messagekey": "permrole-domaindataLabel"
        },
        ".csc-permrole-permission": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-permrole-permissionId": "${{row}.permissionId}",
                                ".csc-permrole-resourceName": "${{row}.resourceName}",
                                ".csc-permrole-actionGroup": "${{row}.actionGroup}"
                            },
                            "type": "fluid.noexpand"
                        }
                    },
                    "elPath": "fields.permission"
                }
            }]
        },
        ".csc-permrole-permission-label": {
            "messagekey": "permrole-permissionLabel"
        }
    },
    "list": {}
}
