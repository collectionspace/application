                "options": {
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-organizationAuthority-contactName": {
                                    "decorators": [{
                                        "func": "cspace.autocomplete",
                                        "type": "fluid",
                                        "options": {
                                            "queryUrl": "../../../tenant/core/vocabularies/organization/autocomplete/contactName",
