<record id="conservation" is-extension="true">
	<services-record-path id="bonsai">conservation_bonsai:http://collectionspace.org/services/conservation/local/bonsai,conservation_bonsai</services-record-path>

	<section id="objectTreatmentInformation">
		<repeat id="treatmentsPerformed" section="bonsai">
			<field id="treatmentPerformed" autocomplete="true" ui-type="enum" section="bonsai" />
		</repeat>
	</section>

	<section id="objectAnalysisInformation">
		<repeat id="futureTreatmentGroupList/futureTreatmentGroup" section="bonsai">
			<field id="futureTreatment" autocomplete="true" ui-type="enum" section="bonsai" />
			<field id="futureTreatmentDate" datatype="date" section="bonsai" />
			<field id="futureTreatmentNote" section="bonsai" />
		</repeat>
	</section>
</record>
