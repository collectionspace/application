<root>
    <include src="extensions/naturalhistory-vocabularies.xml" strip-root="yes" />

    <instance id="vocab-transportmethod">
        <web-url>transportmethod</web-url>
        <title-ref>transportmethod</title-ref>
        <title>Transport Method</title>
        <options>
            <option id="surface">surface</option>
            <option id="hand">hand-carried</option>
            <option id="air">air</option>
        </options>
    </instance>
    <instance id="vocab-herbariumform">
        <web-url>herbariumform</web-url>
        <title-ref>herbariumform</title-ref>
        <title>Herbarium Forms</title>
        <options>
            <option id="unknown">Unknown</option>
            <option id="mounted_on_paper" default="yes">Mounted on Paper</option>
            <option id="loose_in_packet">Loose in packet</option>
            <option id="stored_in_box_bag">Stored in a Box or Bag</option>
            <option id="preserved_in_liquid">Preserved in Liquid</option>
            <option id="microscope_slide_preparation">Microscope Slide Preparation</option>
            <option id="photograph">Photograph</option>
            <option id="illustration">Illustration</option>
            <option id="photocopy">Photocopy</option>
            <option id="box">Box</option>
            <option id="bag">Bag</option>
            <option id="map">Map</option>
        </options>
    </instance>
    <instance id="vocab-taxonmajorgroup">
        <web-url>taxonmajorgroup</web-url>
        <title-ref>taxonmajorgroup</title-ref>
        <title>Major Groups</title>
        <options>
            <option id="spermatophytes">Spermatophytes</option>
            <option id="pteridophytes">Pteridophytes</option>
            <option id="algae">Algae</option>
            <option id="bryophytes">Bryophytes</option>
            <option id="fungi">Fungi</option>
            <option id="lichen">Lichen</option>
            <option id="na">n/a</option>
            <option id="misc">misc</option>
        </options>
    </instance>
    <instance id="vocab-taxonrelatedtermtype">
        <web-url>taxonrelatedtermtype</web-url>
        <title-ref>taxonrelatedtermtype</title-ref>
        <title>Related Term Types</title>
        <options>
            <option id="synonym">Synonym</option>
            <option id="female_hybrid_parent">Female Hybrid Parent</option>
            <option id="male_hybrid_parent">Male Hybrid Parent</option>
        </options>
    </instance>
</root>