            <web-url>person</web-url>
            <title-ref>person</title-ref>
            <title>Local Persons</title>
            <nptAllowed>false</nptAllowed>
        </instance>
        <instance id="person-ulan_pa">
            <web-url>ulan_pa</web-url>
            <title-ref>ulan_pa</title-ref>
