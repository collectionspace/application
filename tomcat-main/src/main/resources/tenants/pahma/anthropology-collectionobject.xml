<record id="collection-object" is-extension="true">
	<services-record-path id="anthropology">collectionobjects_anthropology:http://collectionspace.org/services/collectionobject/domain/anthropology,collectionobjects_anthropology</services-record-path>

	<section id="identificationInformation">
		<field id="homeLocation" autocomplete="location-location" section="anthropology" ui-search="repeatable"/>
		<field id="homeCrate" autocomplete="location-crate" section="anthropology" ui-search="repeatable"/>
		<field id="computedCrate" ui-type="valueDeURNed" services-readonly="true" authref-in-services="true" section="anthropology"/>

		<repeat id="archeologicalProvenienceList" section="anthropology">
			<field id="archeologicalProvenience" section="anthropology">
			</field>
		</repeat>

	</section>

	<section id="descriptionInformation">
		<section id="contentInformation">
			<repeat id="contentWorks" section="anthropology">
				<field id="contentWork" section="anthropology" autocomplete="work-work"/>
			</repeat>
		</section>
	</section>

