	<section id="culturalCareInformation">
		<repeat id="culturalCareNoteList" section="anthropology">
			<field id="culturalCareNote" section="anthropology">
				<selector>collection-object-culturalCareNote</selector>
			</field>
		</repeat>
		<repeat id="accessRestrictionGroupList/accessRestrictionGroup" section="anthropology">
			<field id="accessRestrictionType" section="anthropology">
				<selector>collection-object-accessRestrictionType</selector>
				<options>
					<option id="" default="yes">(none selected)</option>
					<option id="displayVisualRestriction">display/visual</option>
					<option id="handlingGenderRestriction">handling: gender</option> <!-- e.g., "Only men and post-menopausal women should handle this" -->
					<option id="handlingOtherRestriction">handling: other</option> <!-- e.g., "Please do/don't wear gloves when handling this object" -->
					<option id="lendingRestriction">lending</option>
					<option id="publicationRestriction">publication</option>
					<option id="researchAccessRestriction">research/access</option>
					<option id="storageRestriction">storage</option> <!-- e.g., "prefer not stored adjacent to human remains" or "Buddhas should be stored on the top shelf only" -->
					<option id="treatmentRestriction">treatment</option>
					<option id="unknown">unknown</option>
				</options>
			</field>
			<field id="accessRestrictionLevel" section="anthropology">
				<selector>collection-object-accessRestrictionLevel</selector>
				<options>
					<option id="" default="yes">(none selected)</option>
					<option id="preference">preference</option>
					<option id="recommendation">recommendation</option>
					<option id="restriction">restriction</option>
					<option id="unknown">unknown</option>
				</options>
			</field>
			<field id="accessRestrictionDetails" section="anthropology">
				<selector>collection-object-accessRestrictionDetails</selector>
			</field>
			<field id="accessRestrictionRequestor" section="anthropology" autocomplete="person-person">
				<selector>collection-object-accessRestrictionRequestor</selector>
			</field>
			<field id="accessRestrictionOnBehalfOf" section="anthropology" autocomplete="organization-organization">
				<selector>collection-object-accessRestrictionOnBehalfOf</selector>
			</field>
			<field id="accessRestrictionDate" section="anthropology" ui-type="date">
				<!-- FIXME: This field is defined as a string in the database. At some point we should add datatype="date" here, and convert that column to a date. -->
				<selector>collection-object-accessRestrictionDate</selector>
			</field>
		</repeat> 
	</section>
