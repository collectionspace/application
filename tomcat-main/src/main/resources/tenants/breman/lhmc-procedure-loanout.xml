<record id="loanout" is-extension="true">
	<services-record-path id="lhmc">loansout_lhmc:http://collectionspace.org/services/loanout/domain/lhmc,loansout_lhmc</services-record-path>

	<section id="loanoutInformation">
		<repeat id="loanoutStatusLHMCGroupList/loanoutStatusLHMCGroup" section="lhmc">
			<field id="loanoutStatusLHMC" autocomplete="true" ui-type="enum" section="lhmc" />
			<field id="loanoutStatusDateLHMC" datatype="date" section="lhmc" />
			<field id="loanoutStatusNoteLHMC" section="lhmc" />
			<field id="loanoutStatusAuthorizerLHMC" autocomplete="true" section="lhmc" />
		</repeat>
	</section>
</record>
