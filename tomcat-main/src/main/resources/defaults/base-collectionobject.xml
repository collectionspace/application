
<record id="collection-object" in-findedit="yes">
  <services-url>collectionobjects</services-url>
  <services-tenant-singular>CollectionObject</services-tenant-singular>
  <services-list-path>abstract-common-list/list-item</services-list-path>
  <services-record-path id="common"
    >collectionobjects_common:http://collectionspace.org/services/collectionobject,collectionobjects_common</services-record-path>
  <services-record-path id="collectionspace_core"
    >collectionspace_core:http://collectionspace.org/collectionspace_core/,collectionspace_core</services-record-path>
  <web-url>cataloging</web-url>
  <tab-url>cataloging-tab</tab-url>
  <number-selector>.csc-entry-number</number-selector>
  <row-selector>.csc-recordList-row:</row-selector>
  <list-key>cataloging</list-key>
  <hassoftdelete>true</hassoftdelete>
  <structures>
    <structure id="search">
      <view>
        <label-section id="messagekeys" show="true"/>
      </view>
    </structure>
    <structure id="screen">
      <view>
        <label-section id="messagekeys" show="true"/>
        <hierarchy-section show="true">
          <options>
						<!-- The values here may not be used, but are here for completeness -->
            <option id="narrowerContext">collection-object</option>
            <option id="broaderContext">collection-object</option>
          </options>
        </hierarchy-section>
      </view>
    </structure>
    <structure id="tab">
      <view>
        <hierarchy-section show="true">
          <options>
            <!-- The values here may not be used, but are here for completeness -->
            <option id="narrowerContext">collection-object</option>
            <option id="broaderContext">collection-object</option>
          </options>
        </hierarchy-section>
        <titlebar show="false"/>
        <sidebar show="false"/>
        <edit-section id="details" show="true"/>
        <list-section id="list" show="false"/>
        <label-section id="messagekeys" show="true"/>
      </view>
      <repeat id="list" asSibling="true">
        <selector>recordList-row:</selector>
        <field id="summary" ui-type="list">
          <selector>related-summary</selector>
        </field>
        <field id="number" ui-type="list">
          <selector>related-number</selector>
        </field>
      </repeat>
    </structure>
  </structures>

  <include src="domain-collectionobject.xml" strip-root="yes"/>

  <section id="coreInformation">
    <field id="tenantId" section="collectionspace_core" attributes="GET"/>
		<!-- Need to glean csid for termCompletion -->
    <field id="csid" mini="search,list" attributes="GET"></field>
    <field id="refName" section="collectionspace_core" mini="search,list,relate" attributes="GET"></field>
    <field id="createdAt" section="collectionspace_core" attributes="GET"/>
    <field id="updatedAt" ui-search="range" ui-type="date" section="collectionspace_core"
      attributes="GET" mini="search,list,relate"/>
    <field id="createdBy" section="collectionspace_core" attributes="GET"/>
    <field id="updatedBy" ui-search="repeatable" section="collectionspace_core" attributes="GET"/>
    <field id="nametitle" mini="summary,search,list,relate" exists-in-services="false">
      <selector>object-identification-object-nametitle</selector>
      <merges>
        <merge id="title" rank="1"/>
        <merge id="objectName" rank="2"/>
      </merges>
    </field>
  </section>
  <section id="identificationInformation">
    <field id="objectNumber" ui-search="repeatable" chooser="yes" in-title="yes" in-tab="yes"
      display-name="yes" mini="number,search,list,relate" seperate_ui_container="true">
      <selector>object-identification-object-number</selector>
      <title-selector>titleBar-object-identification-object-number</title-selector>
      <options>
        <option id="intake" sample="IN2009.1.1">Intake</option>
        <option id="accession" sample="2009.1.1">Accession</option>
        <option id="loanin" sample="LI2009.1.1">Loans In</option>
      </options>
    </field>
    <field id="numberOfObjects" ui-search="range" ui-type="validated" datatype="integer">
      <selector>object-identification-number-objects</selector>
    </field>
    <repeat id="otherNumberList/otherNumber">
      <selector>object-identification-otherNumbers</selector>
      <field id="numberValue" in-tab="yes">
        <selector>object-identification-other-number</selector>
      </field>
      <field id="numberType" in-tab="yes" seperate_ui_container="true">
        <selector>object-identification-other-number-type</selector>
        <options>
          <option id="">Please select a value</option>
          <option id="lender">Lender</option>
          <option id="obsolete">Obsolete</option>
          <option id="previous">Previous</option>
          <option id="serial">Serial</option>
          <option id="unknown">Unknown</option>
        </options>
      </field>
    </repeat>
    <repeat id="responsibleDepartments" has-primary="yes">
      <selector>object-identification-responsible-department</selector>
      <field id="responsibleDepartment" ui-search="repeatable" mini="search"
        seperate_ui_container="true">
        <selector>object-identification-responsible-department</selector>
        <options>
          <option id="">Please select a value</option>
          <option id="antiquities">Antiquities</option>
          <option id="architecture-design">Architecture and Design</option>
          <option id="decorative-arts">Decorative Arts</option>
          <option id="ethnography">Ethnography</option>
          <option id="herpetology">Herpetology</option>
          <option id="media-performance-art">Media and Performance Art</option>
          <option id="paintings-sculpture">Paintings and Sculpture</option>
          <option id="paleobotany">Paleobotany</option>
          <option id="photographs">Photographs</option>
          <option id="prints-drawings">Prints and Drawings</option>
        </options>
      </field>
    </repeat>
    <field id="collection" ui-search="repeatable" seperate_ui_container="true">
      <selector>object-identification-collection</selector>
      <options>
        <option id="">Please select a value</option>
        <option id="library-collection">Library collection</option>
        <option id="permanent-collection">Permanent collection</option>
        <option id="study-collection">Study collection</option>
        <option id="teaching-collection">Teaching collection</option>
      </options>
    </field>
    <field id="recordStatus" ui-search="repeatable" seperate_ui_container="true">
      <selector>object-identification-record-status</selector>
      <options>
        <option id="approved">Approved</option>
        <option id="in-process">In process</option>
        <option id="new" default="yes">New</option>
        <option id="temporary">Temporary</option>
      </options>
    </field>
    <repeat id="briefDescriptions" has-primary="yes">
      <selector>object-identification-brief-description</selector>
      <field id="briefDescription" datatype="largetext">
        <selector>object-identification-brief-description</selector>
      </field>
    </repeat>
    <field id="distinguishingFeatures">
      <selector>object-identification-distinguishing-features</selector>
    </field>
    <repeat id="comments">
      <field id="comment" datatype="largetext">
        <selector>object-identification-comments</selector>
      </field>
    </repeat>
    <field id="computedCurrentLocation" ui-type="valueDeURNed">
    </field>
    <repeat id="titleGroupList/titleGroup">
      <!-- selector>collection-object-titleGroup</selector -->
      <field id="title" ui-search="repeatable" in-title="yes" mini="search">
        <title-selector>titleBar-object-identification-object-title</title-selector>
        <selector>object-identification-object-title</selector>
      </field>
      <field id="titleLanguage" autocomplete="vocab-languages" ui-type="enum">
        <enum>
          <default>urn:cspace:core.collectionspace.org:vocabularies:name(languages):item:name(eng)'English'</default>
        </enum>
