

            <record id="vocab" in-recordlist="no" type="authority">
                <terms-used>false</terms-used>
                <refobj-used>false</refobj-used>
                <web-url>vocab</web-url>
                <services-tenant-auth-plural>Vocabularies</services-tenant-auth-plural>
                <services-tenant-auth-singular>Vocabulary</services-tenant-auth-singular>
                <services-tenant-singular>Vocabularyitem</services-tenant-singular>
                <services-instances-path>vocabularies_common:http://collectionspace.org/services/vocabulary,abstract-common-list/list-item</services-instances-path>
                <services-single-instance-path>vocabularies_common:http://collectionspace.org/services/vocabulary,vocabularies_common</services-single-instance-path>
                <services-list-path>http://collectionspace.org/services/vocabulary,abstract-common-list/list-item</services-list-path>
                <services-record-path>vocabularyitems_common:http://collectionspace.org/services/vocabulary,vocabularyitems_common</services-record-path>
                <services-record-path id="collectionspace_core">collectionspace_core:http://collectionspace.org/collectionspace_core/,collectionspace_core</services-record-path>
                <services-url>vocabularies</services-url>
                <authority-vocab-type>enum</authority-vocab-type>
                <urn-syntax>urn:cspace:org.collectionspace.demo:vocabulary:{vocab}:item:{entry}'{display}'</urn-syntax>
                <hassoftdelete>true</hassoftdelete>
                <structures>
                    <structure id="screen"></structure>
                </structures>
				
				<include src="domain-vocabularies.xml" />
				
                <instances>
                    <instance id="vocab-xxx">
                        <web-url>xxx</web-url>
                        <title-ref>xxx</title-ref>
                        <title>Temporary Vocab whilst none exist</title>
                    </instance>
                    <instance id="vocab-languages">
                        <web-url>languages</web-url>
                        <title-ref>languages</title-ref>
                        <title>My first list</title>
                        <options>
                            <option id="eng">English</option>
                            <option id="spn">Spanish</option>
                            <option>French</option>
                            <option>Italian</option>
                            <option>Portuguese</option>
                            <option>German</option>
                            <option>Russian</option>
                            <option>Arabic</option>
                            <option>Chinese</option>
                            <option>Hebrew</option>
                            <option>Japanese</option>
                            <option>Korean</option>
                            <option>Swahili</option>
                            <option>Ancient Greek</option>
                            <option>Latin</option>
                            <option>Old English</option>
                            <option>Middle English</option>
                        </options>
                    </instance>
                    <instance id="vocab-lostatus">
                        <web-url>loanoutstatus</web-url>
                        <title-ref>loanoutstatus</title-ref>
                        <title>loanedObjectStatus</title>
                        <options>
                            <option>Requested</option>
                            <option>Authorized</option>
                            <option>Agreed</option>
                            <option>Conservation</option>
                            <option>Sent</option>
                            <option>Photography requested</option>
                            <option>Refused</option>
                            <option>Renewed</option>
                            <option>Returned</option>
                        </options>
                    </instance>
                    <instance id="vocab-currency">
                        <web-url>currency</web-url>
                        <title-ref>currency</title-ref>
                        <title>Standard Currency</title>
                        <options>
                            <option id="USD">US Dollar</option>
                            <option>Canadian Dollar</option>
                            <option id="EUR">Euro</option>
                            <option id="GBP">Pound Sterling</option>
                            <option>Danish Krone</option>
                            <option>Swedish Krona</option>
                            <option>Swiss Franc</option>
                        </options>
                    </instance>
                    <instance id="vocab-collectionmethod">
                        <web-url>collectionmethod</web-url>
                        <title-ref>collectionmethod</title-ref>
                        <title>Collection Method</title>
                        <options>
                            <option id="borehole">borehole</option>
                            <option id="commissioned">commissioned</option>
                            <option id="exchanged">exchanged</option>
                            <option id="founddead">found dead</option>
                            <option id="metaldetector">metal detector</option>
                            <option id="netted">netted</option>
                            <option id="photographed">photographed</option>
                            <option id="purchased">purchased</option>
                            <option id="salvaged">salvaged</option>
                            <option id="shot">shot</option>
                            <option id="trapped">trapped</option>
                        </options>
                    </instance>
                    <instance id="vocab-conditionfitness">
                        <web-url>conditionfitness</web-url>
                        <title-ref>conditionfitness</title-ref>
                        <title>Condition Fitness</title>
                        <options>
                            <option id="dangerous">Dangerous</option>
                            <option id="suitable">Suitable</option>
                            <option id="reasonable">Reasonable</option>
                            <option id="unsuitable">Unsuitable</option>
                        </options>
                    </instance>
                    <instance id="vocab-conditioncheckmethod">
                        <web-url>conditioncheckmethod</web-url>
                        <title-ref>conditioncheckmethod</title-ref>
                        <title>Condition Check Method</title>
                        <options>
                            <option id="observed">Observed</option>
                            <option id="xrayed">X-Rayed</option>
                        </options>
                    </instance>
                    <instance id="vocab-entrymethod">
                        <web-url>entrymethod</web-url>
                        <title-ref>entrymethod</title-ref>
                        <title>Entry Method</title>
                        <options>
                            <option id="inperson">In person</option>
                            <option id="post">Post</option>
                            <option id="foundondoorstep">Found on doorstep</option>
                        </options>
                    </instance>
                    <instance id="vocab-conditioncheckreason">
                        <web-url>conditioncheckreason</web-url>
                        <title-ref>conditioncheckreason</title-ref>
                        <title>Condition Check Reason</title>
                        <options>
                            <option id="consideration">Consideration</option>
                            <option id="damagedintransit">Damaged in transit</option>
                            <option id="enquiry">Enquiry</option>
                            <option id="loanin">Loan in</option>
                            <option id="conservation">Conservation</option>
                        </options>
                    </instance>
                    <instance id="vocab-addresstype">
                        <web-url>addresstype</web-url>
                        <title-ref>addresstype</title-ref>
                        <title>Contact Address Type</title>
                        <options>
                            <option id="alternative">Alternative</option>
                            <option id="mailing">Mailing</option>
                            <option id="previous">Previous</option>
                            <option id="street">Street</option>
                        </options>
                    </instance>
                    <instance id="vocab-locationtype">
                        <web-url>locationtype</web-url>
                        <title-ref>locationtype</title-ref>
                        <title>Location Type</title>
                        <options>
                            <option id="building">Building</option>
                            <option id="room">Room</option>
                            <option id="unit">Unit</option>
                            <option id="shelf">Shelf</option>
                            <option id="drawer">Drawer</option>
                            <option id="tray">Tray</option>
                        </options>
                    </instance>
                    <instance id="vocab-dateera">
                        <web-url>dateera</web-url>
                        <title-ref>dateera</title-ref>
                        <title>Date Era</title>
                        <options>
                            <option id="ce">CE</option>
                            <option id="bce">BCE</option>
                        </options>
                    </instance>
                    <instance id="vocab-datequalifier">
                        <web-url>datequalifier</web-url>
                        <title-ref>datequalifier</title-ref>
                        <title>Date Qualifier</title>
                        <options>
                            <option id="days">Day(s)</option>
                            <option id="month">Month(s)</option>
                            <option id="years">Year(s)</option>
                        </options>
                    </instance>
                    <instance id="vocab-dateCertainty">
                        <web-url>datecertainty</web-url>
                        <title-ref>datecertainty</title-ref>
                        <title>Date Certainty</title>
                        <options>
                            <option id="after">After</option>
                            <option id="approximate">Approximate</option>
                            <option id="before">Before</option>
                            <option id="circa">Circa</option>
                            <option id="probably">Probably</option> 
                            <option id="possibly">Possibly</option>
                        </options>
                    </instance>
                </instances>
                <section>
                    <field id="displayName" in-title="yes" display-name="yes" mini="summary,number,search,list,relate">
                        <selector>vocabulary-displayName</selector>
                        <title-selector>titleBar-displayName</title-selector>
                    </field>
                    <field id="termStatus" seperate_ui_container="true">
                        <selector>vocabulary-status</selector>
                        <options>
                            <option id="provisional" default="yes">Provisional</option>
                            <option id="under-review">Under Review</option>
                            <option id="accepted">Accepted</option>
                            <option id="rejected">Rejected</option>
                        </options>
                    </field>
                    <field id="shortIdentifier">
                        <selector>vocabulary-shortIdentifier</selector>
                    </field>
    				<field id="description"></field>
                </section>
            </record>
