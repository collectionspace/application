<record id="dimension" in-recordlist="no" separate-record="false" services-type="measuredPartGroupList">
	<section id="dimensionInformation">
		<repeat id="measuredPartGroupList/measuredPartGroup">
			<field id="dimensionSummary" />
			<field id="measuredPart" />
			<repeat id="dimensionSubGroupList/dimensionSubGroup">
				<field id="measuredBy" autocomplete="true" />
				<field id="dimension" />
				<field id="measurementUnit" />
				<!-- BAMPFA-208: Hacking the common schema, since cspace doesn't handle extensions on the dimension record properly. -->
				<!-- Change value to a string. -->
				<!-- <field id="value" datatype="float" /> -->
				<field id="value"></field>
				<field id="measurementMethod" />
				<field id="valueDate" datatype="date" />
				<field id="valueQualifier" />
				<field id="dimensionNote" />
			</repeat>
		</repeat>
	</section>
</record>
